{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/analyze",
      "destination": "https://verifychain-rwa.onrender.com/analyze_and_oracle"
    },
    {
      "source": "/api/health",
      "destination": "https://verifychain-rwa.onrender.com/health"
    }
  ],
  "redirects": [
    {
      "source": "/app",
      "destination": "/home",
      "permanent": true
    },
    {
      "source": "/dashboard",
      "destination": "/home",
      "permanent": true
    }
  ]
}
